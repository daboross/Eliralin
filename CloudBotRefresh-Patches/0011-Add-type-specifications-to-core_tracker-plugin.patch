From 87de562fc63ac92b694b4c92373ded22cb173173 Mon Sep 17 00:00:00 2001
From: Dabo Ross <daboross@daboross.net>
Date: Sun, 13 Apr 2014 14:59:46 -0700
Subject: [PATCH] Add type specifications to core_tracker plugin


diff --git a/plugins/core_tracker.py b/plugins/core_tracker.py
index d02a65e..1c72aeb 100644
--- a/plugins/core_tracker.py
+++ b/plugins/core_tracker.py
@@ -9,19 +9,30 @@ nick_re = re.compile(":(.+?)!")
 
 
 @hook.event("KICK")
-def on_kick(paraml, conn=None, chan=None):
+def on_kick(paramlist, conn=None, chan=None):
+    """
+    :type paramlist: list[str]
+    :type conn: core.irc.BotConnection
+    :type chan: str
+    """
     # if the bot has been kicked, remove from the channel list
-    if paraml[1] == conn.nick:
+    if paramlist[1] == conn.nick:
         conn.channels.remove(chan)
         auto_rejoin = conn.config.get('auto_rejoin', False)
         if auto_rejoin:
-            conn.join(paraml[0])
+            conn.join(paramlist[0])
 
 
 @hook.event("NICK")
-def on_nick(paraml, bot=None, conn=None, raw=None):
+def on_nick(paramlist, bot=None, conn=None, raw=None):
+    """
+    :type paramlist: list[str]
+    :type bot: core.bot.CloudBot
+    :type conn: core.irc.BotConnection
+    :type raw: str
+    """
     old_nick = nick_re.search(raw).group(1)
-    new_nick = str(paraml[0])
+    new_nick = str(paramlist[0])
     if old_nick == conn.nick:
         conn.nick = new_nick
         bot.logger.info("Bot nick changed from '{}' to '{}'.".format(old_nick, new_nick))
-- 
